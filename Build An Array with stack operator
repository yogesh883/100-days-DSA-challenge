import java.util.*;

class Solution {
    public List<String> buildArray(int[] target, int n) {
        List<String> operations = new ArrayList<>();
        int index = 0;

        for (int i = 1; i <= n && index < target.length; i++) {
            operations.add("Push");

            if (i == target[index]) {
                index++;          // keep it
            } else {
                operations.add("Pop"); // remove it
            }
        }
        return operations;
    }
}
